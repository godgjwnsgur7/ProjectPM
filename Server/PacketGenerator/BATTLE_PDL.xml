<?xml version="1.0" encoding="utf-8" ?>
<PDL>
	<packet name="RES_CONNECTED">
	</packet>
	<packet name="REQ_ENTER_GAME">
		<int name="characterType"/>
		<int name="roomId"/>
		<int name="roomMemberCount"/>
	</packet>
	<packet name="RES_BROADCAST_ENTER_GAME">
		<int name="playerId"/>
	</packet>
	<packet name="REQ_LEAVE_GAME">
	</packet>
	<packet name="RES_BROADCAST_LEAVE_GAME">
		<int name="playerId"/>
	</packet>
	<packet name="REQ_PLAYER_LIST">
	</packet>
	<packet name="RES_PLAYER_LIST">
		<list name="player">
			<bool name="isSelf"/>
			<int name="characterType"/>
			<int name="playerId"/>
		</list>
	</packet>
	<packet name="REQ_FRAME_INPUT">
		<int name="frameNumber"/>
		<int name="playerId"/>
		<float name="moveX"/>
		<float name="moveY"/>
		<int name="attackKey"/>
		<bool name="isJump"/>
		<bool name="isGuard"/>
		<int name="damageType"/>
		<bool name="isSuccessAttack"/>
	</packet>
	<packet name="RES_FRAME_INPUT">
		<int name="frameNumber"/>
		<list name="playerInput">
			<int name="playerId"/>
			<float name="moveX"/>
			<float name="moveY"/>
			<int name="attackKey"/>
			<bool name="isJump"/>
			<bool name="isGuard"/>
			<int name="damageType"/>
			<bool name="isSuccessAttack"/>
		</list>
	</packet>
			
</PDL>